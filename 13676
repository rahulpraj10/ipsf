// Move File
function Service_PreInvokeMethod (MethodName, Inputs, Outputs)
{
	var msg = COMCreateObject("WScript.Shell");
	var sBC;
	
	if(MethodName == "MoveFileToTemp"){
	    var sFileReturn="";
		var sSDR;
		var sDDR;
		var sImgArray = new Array();
		var i=0;
		var parentId = Inputs.GetProperty("ParentId");
		//msg.Popup("Hey Got U BS : "+parentId);
        var moveSuccess;
		var deleteSuccess;
        sBC = TheApplication().GetBusObject("Auto Vehicle").GetBusComp("Asset Mgmt - Asset Attachment");
		with(sBC){
          ClearToQuery();
		  SetViewMode(AllView);
		  SetSearchSpec("Asset Id", parentId);
		  ExecuteQuery(ForwardOnly);
		  //msg.Popup(CountRecords());
		  var isRec = FirstRecord();
			  while(isRec){
			  sFileReturn = InvokeMethod("GetFile","AssetFileName");
			  sImgArray[i] = sFileReturn.substring(sFileReturn.lastIndexOf("\\")+1);
              sSDR = sFileReturn.substring(sFileReturn.indexOf(",")+1);
			  sDDR = "D:\\Siebel_IP15\\Client\\PUBLIC\\enu\\IMAGES\\custom\\"+sImgArray[i];
              moveSuccess = SElib.dynamicLink("Kernel32.dll","MoveFileA",STDCALL,sSDR,sDDR);
			  //msg.Popup(moveSuccess);
                 
                   if(!moveSuccess){
				     deleteSuccess = SElib.dynamicLink("Kernel32.dll","DeleteFileA",STDCALL,sSDR);
					 //msg.Popup(deleteSuccess);
				   }

			  //msg.Popup(sImgArray[i]);
			  i++;
			  isRec = NextRecord();
			  }
          Outputs.SetProperty("ImgArray",sImgArray);  
		 
		
		}

    return (CancelOperation);

	}
	if(MethodName == "ShowBIP"){
	var sAttId = Inputs.GetProperty("AttId");
	//msg.Popup(sAttId);
    sBC = TheApplication().GetBusObject("Report Administration").GetBusComp("Report Output BC");
 		with(sBC){
          ClearToQuery();
		  SetViewMode(AllView);
		  SetSearchSpec("Id", sAttId);
		  ExecuteQuery(ForwardOnly);
		  //msg.Popup(CountRecords());
		  if(FirstRecord()){
		   sFileReturn = InvokeMethod("GetFile","ReportOutputFileName");
		   //msg.Popup(sFileReturn);
		   var sAbs = sFileReturn.substring(sFileReturn.indexOf(",")+1);		   
		   var sFileName = sFileReturn.substring(sFileReturn.lastIndexOf("\\")+1);
		   //msg.Popup(sFileName+" : "+sAbs);
		   var sDestinationDir = "D:\\Siebel_IP15\\FS";
		   var s = 'copy "'+sAbs+'" "'+sDestinationDir+'"';  
		   //msg.Popup(s);
		   var iReturnVal = Clib.system(s);		  
		   //msg.Popup(iReturnVal);
		   Outputs.SetProperty("FileName",sFileName); 
		  }		 
		}
	
    return (CancelOperation);	
	}
	return (ContinueOperation);
}
