// combined Query
select a.X_AGREE_ID,a.X_TYPE,b.ROW_ID from siebel.CX_SUPPLY_CTLG a,siebel.S_DOC_AGREE b where a.X_TYPE='Adhoc' and a.x_agree_id=b.row_id

select a.X_AGREE_ID,a.X_TYPE,b.row_ID from siebel.CX_Supply_CTLG a join siebel.S_DOC_AGREE b on a.x_agree_id=b.row_id

