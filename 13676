// Google Transalte PM
if( typeof( SiebelAppFacade.GoogleTranslatePM ) === "undefined" ){
    SiebelJS.Namespace( "SiebelAppFacade.GoogleTranslatePM" );
    define("siebel/custom/googletranslatepm", [], function () {
        SiebelAppFacade.GoogleTranslatePM = ( function(){
            function GoogleTranslatePM( proxy ){
                SiebelAppFacade.GoogleTranslatePM.superclass.constructor.call( this, proxy );
            }
            SiebelJS.Extend( GoogleTranslatePM, SiebelAppFacade.PresentationModel );
            GoogleTranslatePM.prototype.Init = function(){
                SiebelAppFacade.GoogleTranslatePM.superclass.Init.call( this );
                this.AddProperty( "DescriptionValue", "" );
				this.AddMethod( "ShowSelection",  SelectionChange, { sequence : false, scope : this } );
                this.AddMethod( "FieldChange",  OnFieldChange, { sequence : false, scope: this } );
            };
            function SelectionChange(){
                var controls = this.Get( "GetControls" );
                var control = controls[ "Description" ];
                var value = this.ExecuteMethod( "GetFieldValue", control );
                this.SetProperty( "DescriptionValue", value );
            }
            function OnFieldChange( control, value ){
                if( control.GetName() === "Description" ){
					this.SetProperty( "DescriptionValue", value );
                }
            }
            return GoogleTranslatePM;
        } ());
        return "SiebelAppFacade.GoogleTranslatePM";
    });
}
// Google Transalte PR
if( typeof( SiebelAppFacade.GoogleTranslatePR ) === "undefined" ){
    SiebelJS.Namespace( "SiebelAppFacade.GoogleTranslatePR" );
    define("siebel/custom/googletranslatepr", ["siebel/phyrenderer"], function () {
        SiebelAppFacade.GoogleTranslatePR = ( function(){
            function GoogleTranslatePR( pm ){
                SiebelAppFacade.GoogleTranslatePR.superclass.constructor.call( this, pm );
            }
            SiebelJS.Extend( GoogleTranslatePR, SiebelAppFacade.PhysicalRenderer );
            GoogleTranslatePR.prototype.ShowUI = function () {
                SiebelAppFacade.GoogleTranslatePR.superclass.Init.call(this);
                this.AttachPMBinding( "DescriptionValue", Translate );
				//Div for displaying translated text
				var sTranslatedTextDiv = '<div class="mceGridField"><textarea id="translatedText" style="height:24px;width:120px;" readonly="readonly"></textarea></div>';
				var sDescriptionInputName = SiebelApp.S_App.GetActiveView().GetApplet("Activity Form Applet").GetControl("Description").GetInputName();
				$("[name='"+sDescriptionInputName+"']").after(sTranslatedTextDiv);
            };
            function Translate(){
				var sDescription = this.GetPM().Get("DescriptionValue");
				var sSourceLanguage = "en";
				var sTargetLanguage = "ja";
				var sAPIKey = "AIzaSyDb47LfWN46fy6CUNKnQxEAZRoUZyDVUrA";
				var sGoogleTranslateURL = 'https://www.googleapis.com/language/translate/v2?key='+sAPIKey+'&source='+sSourceLanguage+'&target='+sTargetLanguage+'&q='+sDescription;
				$.ajax(
				{
					url: sGoogleTranslateURL,
					type: 'GET',
					success: function(results)
					{
						var sTranslatedText = results.data.translations[0].translatedText;
						console.log("Return Value : "+results);
						console.log("Translated Text is : "+sTranslatedText);
						$("#translatedText").val(sTranslatedText);
					},
					error: function(xhr, status, error)
					{
						//console.log("Error: " +error);
					}
				});
            }
            return GoogleTranslatePR;
        } ());
        return "SiebelAppFacade.GoogleTranslatePR";
    });
}
