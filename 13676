//  Directive in AngularJS.
→ Move to the next element, after filling one input (automatic tabing)
<input type="text" id="part1" class="form-control input" ng-model="myObject.part1" maxlength="1" move-next-on-maxlength / style="width:15% !important"> 

angular.module('angularGanttDemoApp')
.directive("moveNextOnMaxlength", function() {
    return {
        restrict: "A",
        link: function($scope, element) {
            element.on("input", function(e) {
                if(element.val().length == element.attr("maxlength")) {
                    var $nextElement = element.next();
                    if($nextElement.length) {
                        $nextElement[0].focus();
                    }
                }
            });
        }
    }
}); 

