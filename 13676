  function () {
   SiebelAppFacade.LBSMADCSRPR = (function () {
    function LBSMADCSRPR(pm) {
     SiebelAppFacade.LBSMADCSRPR.superclass.constructor.apply(this, arguments);
    }
    SiebelJS.Extend(LBSMADCSRPR, SiebelAppFacade.PhysicalRenderer);
	LBSMADCSRPR.prototype.ShowUI = function () {  
     SiebelAppFacade.LBSMADCSRPR.superclass.ShowUI.apply(this, arguments);
	 var customdiv1='<td nowrap="" valign="middle" colspan="1" rowspan="1"><div class="mceGridLabel" align="Right" style="font-size:11px;"><span id="LXK_LBS_Office_Label">Move To Office:</span><br/><br/></div></td>';
	 var customdiv2 = '<div class="mceGridField"><input type="text" name="OfficeText" value="" id="MoveToOffice" aria-labelledby="LXK_LBS_Office_Label" aria-label="Move To Office" style="height: 24px; width: 119.777777671814px;" maxlength="30" tabindex="0" class="siebui-input-align-left" data-seq="0" aria-readonly="true"></div>';
