// Salutation

if( typeof( SiebelAppFacade.SalutationPR ) === "undefined" ){
    SiebelJS.Namespace( "SiebelAppFacade.SalutationPR" ); 
    define("siebel/custom/salutationpr", [ "siebel/phyrenderer" ], function () {
    SiebelAppFacade.SalutationPR = ( function(){
        var siebConsts  = SiebelJS.Dependency( "SiebelApp.Constants" ); 
        function SalutationPR( pm ){
            SiebelAppFacade.SalutationPR.superclass.constructor.call( this, pm );	
		}
      SiebelJS.Extend( SalutationPR, SiebelAppFacade.PhysicalRenderer );
		SalutationPR.prototype.ShowUI = function(){
			SiebelAppFacade.SalutationPR.superclass.ShowUI.call( this );
			var pm = this.GetPM();
			var placeHolder = pm.Get( "GetPlaceholder" );  
			var Controls = pm.Get("GetControls");
			var Title = Controls["AppletTitle"];
			FullId = pm.Get("GetFullId");
			$("#s_" + FullId + "_div").html( '<div class="salutation-pr-applet" id="' + placeHolder + '" ></div>' );
			$("#" + placeHolder).append('<span class="salutation-pr-title">' + Title.GetDisplayName() + '</span>');
		}
		SalutationPR.prototype.BindEvents = function () {
		}
		SalutationPR.prototype.BindData = function(){
			var pm = this.GetPM();
			var recordSet = pm.Get( "GetRecordSet" );
			var recordLength = recordSet.length;
			var Salutation = "";
			for( var i = 0; i < recordLength; i++ ){
					objRS = recordSet[i];
					Salutation += objRS["Result Text"];
			}
			var placeHolder = pm.Get( "GetPlaceholder" );  
			var LoginName = SiebelApp.S_App.GetUserName();
			//var imgPath="https://dlexwsbla011.na.ds.lexmark.com:9001/profilepic/"+LoginName+".jpg";
			var imgPath="IMAGES/Profile/"+LoginName+".jpg";
			var customdiv='<div  class="photo-grid" ><figure id="myFigure"><img align="left" src=imgPath id="myImage" alt="Profile Pic" style="width:100px;height:100px"><figcaption align="center" style="width:110px;height:110px"></figcaption></figure></div>';
			var origDiv = $('.salutation-pr-applet').attr('id');
			$("#"+origDiv).append(customdiv);
			$("#" + placeHolder).append('<span class="salutation-pr-salutation">' + Salutation + '</span>');
			$("#myImage").error(function(){
			$("#myImage").attr("src",imgPath);
				$("#myImage").error(function(){	
						//document.getElementById("myImage").src = "https://dlexwsbla011.na.ds.lexmark.com:9001/profilepic/default.png";
						document.getElementById("myImage").src = "IMAGES/Profile/Default.jpg";
				});
			});
		};	
		return SalutationPR;
	}());
	return "SiebelAppFacade.SalutationPR";
   });
   }
   
