// Goto Particular Record In OpenUI
function GotoSRView(sAppletName,sViewName,sSRId,sAccountId)
			{
				$("#filterDefLat").val(SiebelApp.S_App.GetBusObj().GetBusCompByName("LXK LBS Asset Mgmt - Asset").GetFieldValue("LXK LBS Latitude"));
				$("#filterDefLng").val(SiebelApp.S_App.GetBusObj().GetBusCompByName("LXK LBS Asset Mgmt - Asset").GetFieldValue("LXK LBS Longitude"));
				SiebelApp.S_App.SetProfileAttr("LBSGotoMapAccountId",sAccountId);
				StoreFilterValues();
				var oGotoViewBS = SiebelApp.S_App.GetService("FINS Goto View Service");
				var sGotoViewInputPS = SiebelApp.S_App.NewPropertySet();
				var sGotoViewOutputPS = SiebelApp.S_App.NewPropertySet();
				if(oGotoViewBS)
				{
					sGotoViewInputPS.SetProperty("AppletName",sAppletName);
					sGotoViewInputPS.SetProperty("ViewName",sViewName);
					sGotoViewInputPS.SetProperty("RowId",sSRId);
					sGotoViewOutputPS = oGotoViewBS.InvokeMethod("GotoView", sGotoViewInputPS);
				}
			}
            return LBSAssetMapPR;

