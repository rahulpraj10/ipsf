	 $("#LXK_LBS_Latitude_Label").before(customdiv1);
	 $("[name='"+sTextFieldName+"']").before(customdiv2);
//Setting the field values	
	 var oBS = SiebelApp.S_App.GetService("Inbound E-mail Database Operations");
	 if(oBS){
		 var sInputPS = SiebelApp.S_App.NewPropertySet();
		 sInputPS.SetProperty("BusComp","Service Request");
		 sInputPS.SetProperty("BusObj","Service Request");
		 sInputPS.SetProperty("Id","1DA-HIL");
		 sInputPS.SetProperty("Field: LXK MPS Inside Location","S3TCW11");
		 //sInputPS.SetProperty("Field: LXK MPS IP Address","10.42.8.55"); //Any Number of Fields can be updated.
		 oBS.InvokeMethod("UpdateRecord", sInputPS);
			}
//Gettting the field values.
	 var sBS = SiebelApp.S_App.GetService("Inbound E-mail Database Operations");
	 if(sBS){
		 var sInputPS = SiebelApp.S_App.NewPropertySet();
		 sInputPS.SetProperty("BusComp","Service Request");
		 sInputPS.SetProperty("BusObj","Service Request");
		 sInputPS.SetProperty("IdField","Id");
		 sInputPS.SetProperty("Id","1DA-HIL");
		 sInputPS.SetProperty("ValueFields","LXK MPS Inside Location");
		 //sInputPS.SetProperty("ValueFields","LXK MPS Inside Location,LXK MPS IP Address");//Any Number of Fields can be retrieved.
		 var sOutputPS = sBS.InvokeMethod("GetFieldValues", sInputPS);
		 var sResultSet = sOutputPS.GetChildByType("ResultSet");
		 var op1 = sResultSet.GetProperty("LXK MPS Inside Location");
					if(op1){
					document.getElementById("MoveToOffice").value = op1;
	   					   }
			}
	}
    return LBSMADCSRPR;
   }()
  );
  return "SiebelAppFacade.LBSMADCSRPR";
 })
